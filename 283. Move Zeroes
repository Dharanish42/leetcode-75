class Solution {
    public void moveZeroes(int[] nums) {
        int c=0,j=0,n=nums.length;
        for(int i=0;i<n;i++){
        if(nums[i]!=0)
        {
           nums[j]=nums[i];
           j++;
        }  
        else
        {
            c++;
        }
    }
    int a=n-c;
    for(int i=a;i<n;i++)
    {
        nums[i]=0;
    } 
    }
}
